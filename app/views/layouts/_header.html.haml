#user_panel
  - if current_user
    = link_to "Logout", user_session_path(current_user), :method => :delete
    - if logged_owner?
      = link_to "Profil właściciela", owner_path(current_user.owner)
    - else
      = link_to "Profil użytkownika", user_path(current_user)
  - else
    = link_to "Login", new_user_session_path
    = link_to "Rejestracja", new_user_path